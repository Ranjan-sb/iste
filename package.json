{
    "name": "rstart",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "prepare": "husky install",
        "postinstall": "node -e \"require('fs').copyFileSync('node_modules/pdfjs-dist/build/pdf.worker.min.mjs', 'public/pdf.worker.min.mjs')\"",
        "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
        "db:push": "drizzle-kit push --config=drizzle.config.ts",
        "db:seed": "tsx src/server/db/seed.ts",
        "db:auth:generate": "pnpm dlx @better-auth/cli generate --output src/server/db/auth-schema.ts -y"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.842.0",
        "@aws-sdk/s3-request-presigner": "^3.842.0",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/modifiers": "^9.0.0",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@hookform/resolvers": "^5.1.1",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.5",
        "@radix-ui/react-tabs": "^1.1.12",
        "@tanstack/react-query": "^5.74.3",
        "@trpc/client": "^11.1.0",
        "@trpc/react-query": "^11.1.0",
        "@trpc/server": "^11.1.0",
        "better-auth": "^1.2.7",
        "class-variance-authority": "^0.7.1",
        "client-only": "^0.0.1",
        "clsx": "^2.1.1",
        "dotenv": "^17.0.0",
        "drizzle-orm": "^0.42.0",
        "lodash": "^4.17.21",
        "lucide-react": "^0.488.0",
        "next": "15.3.0",
        "next-themes": "^0.4.6",
        "pdfjs-dist": "^5.3.31",
        "pg": "^8.14.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.59.0",
        "react-pdf": "^10.0.1",
        "resend": "^4.6.0",
        "server-only": "^0.0.1",
        "superjson": "^2.2.2",
        "tailwind-merge": "^3.2.0",
        "tw-animate-css": "^1.2.5",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "@better-auth/cli": "^1.2.7",
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4.1.4",
        "@types/lodash": "^4.17.19",
        "@types/node": "^20",
        "@types/pg": "^8.11.13",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "drizzle-kit": "^0.31.0",
        "eslint": "^9",
        "eslint-config-next": "15.3.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.5",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^4.1.4",
        "tsx": "^4.20.3",
        "typescript": "^5"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "better-sqlite3",
            "esbuild",
            "prisma",
            "sharp"
        ]
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md,json,html,mdx}": "prettier --config ./prettier.config.mjs --write"
    }
}
